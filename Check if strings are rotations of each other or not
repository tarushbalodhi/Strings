bool areRotations(string s1,string s2)
    {
        if(s1.size() != s2.size()) return 0;
        
       string temp = s1 + s1;
       
       auto found = temp.find(s2);    // is s2 is found in temp string 
       
       if(found != string::npos ){
           return 1;
       }
       return 0;
    }
